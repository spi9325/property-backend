
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id        String    @id @default(cuid())
  propertyName     String    @unique
  propertyPrice    Float
  propertyLocation String
  propertyInfo     String
  createdAt DateTime  @default(now())
}
model Template {
  id          String   @id @default(cuid())
  Name    String?   
  Email   String?
  Age     String?
  Projects String?
  Experience String?
  Location String?
  Role     String?
  Title    String?
  Tagline  String?
  Profile  String?
  Image    String?
  Bio      String?
  Social   String?
  Service    String?
  Description String?
  Client   String?
  Review   String?
}