
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id        String    @id @default(cuid())
  propertyName     String    @unique
  propertyPrice    Float
  propertyLocation String
  propertyInfo     String
  createdAt DateTime  @default(now())
}